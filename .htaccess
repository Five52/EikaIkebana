Options -Indexes
<Files .htaccess>
	order allow,deny
	deny from all
</Files>
RedirectMatch 404 /.git
RedirectMatch 404 /config
RedirectMatch 404 /data
RedirectMatch 404 /handler
RedirectMatch 404 /model
RedirectMatch 404 /vendor
RedirectMatch 404 /view
RewriteEngine on
RewriteRule ^(.*)/$ $1 [R=301]
RewriteRule ^([a-zA-Z0-9\-\_\/]*)$ index.php?request=/$1 [QSA]